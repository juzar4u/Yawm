@model PetaPoco.Page<AkhbaarAlYawm.DataAccess.Custom.Entities.MasterMiqaatModel>

<style>
    .main label {
        padding: 20px 25px 14px 19px;
        font-size: 22px;
        font-weight: bold;
        padding-top: 11px;
    }
</style>
<div class="main">

    <input id="tab1" type="radio" name="tabs" checked>
    <label for="tab1">Miqaat List</label>

    <input id="tab2" type="radio" name="tabs">
    <label for="tab2">Calender</label>


    <div class="content" id="content">
     
    <h2 class="separator" style="margin: 20px 0px;">Miqaat</h2>
        <div id="content1">
            <p>
                <div id="projectlisting">@Html.Partial("~/Views/Miqaat/Index.Mobile.cshtml", Model)</div>
            </p>
        </div>

        <div id="content2" style="display: none">
            <p>
                <iframe src="http://www.mumineencalendar.com/" style="width: 100%; height: 571px;"></iframe>
            </p>
        </div>


    </div>

</div>

<script type="text/javascript">
    $("#tab1").click(function () {
        $("#content1").css('display', 'block');
        $("#content2").css('display', 'none');

    });
    $("#tab2").click(function () {
        $("#content2").css('display', 'block');
        $("#content1").css('display', 'none');

    });
</script>



